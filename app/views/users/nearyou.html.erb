<% provide(:title, 'Close to you') %>

<div class = "well gray" >

<h1>Users</h1>
<%= will_paginate %>
<ul class="users">
<% @users.each do |user| %>
<% if user.activated? && !user.admin?  %>
<li>
  <%= image_tag user.avatar(:thumb) %>
  <%= link_to user.name, user %>

  <% if user.chef? %>
    <mark>CHEF</mark>
  <% end %>
</li>
<% end %>
<% end %>
</ul>
<%= will_paginate %>
</div>
